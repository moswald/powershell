{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "alias": "leading bubble",
          "leading_diamond": "\u256d\u2500",
          "template": "<,></>",
          "foreground": "transparent",
          "background": "#26C6DA",
          "type": "text",
          "style": "diamond"
        },
        {
          "foreground": "#111111",
          "background": "#FFD664",
          "type": "upgrade",
          "style": "plain"
        },
        {
          "alias": "os icon",
          "leading_diamond": "\ue0b6",
          "template": "<#26C6DA,transparent> {{ if .WSL }}WSL at {{ end }}{{.Icon}}</>",
          "foreground": "transparent",
          "background": "#26C6DA",
          "type": "os",
          "style": "diamond"
        },
        {
          "alias": "running as root?",
          "trailing_diamond": "\ue0b4",
          "template": "<#FC951E,transparent> \uf0e7 </>",
          "foreground": "transparent",
          "background": "#26C6DA",
          "type": "root",
          "style": "diamond"
        },
        {
          "alias": "devbox indicator",
          "template": "{{ if .Env.IsDevBox }}\ue0b6 DevBox \ue0b4{{ end }}",
          "foreground": "#d83b01",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        },
        {
          "alias": "show [ADO] segment if in ADO subfolder",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "[ADO]",
          "foreground": "transparent",
          "background": "#A5C25C",
          "type": "text",
          "style": "diamond",
          "include_folders": [
            "D:\\\\ado",
            "D:\\\\ado\\\\.*"
          ]
        },
        {
          "alias": "show [ADO 2] segment if in ADO2 subfolder",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "[ADO 2]",
          "foreground": "transparent",
          "background": "#A5C25C",
          "type": "text",
          "style": "diamond",
          "include_folders": [
            "D:\\\\ado2",
            "D:\\\\ado2\\\\.*"
          ]
        },
        {
          "alias": "show [ADO CC] segment if in ADO.ConfigChange subfolder",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "[ADO CC]",
          "foreground": "transparent",
          "background": "#A5C25C",
          "type": "text",
          "style": "diamond",
          "include_folders": [
            "D:\\\\m\\\\projects\\\\MS\\\\ado.configchange",
            "D:\\\\m\\\\projects\\\\MS\\\\ado.configchange\\\\.*"
          ]
        },
        {
          "properties": {
            "folder_separator_icon": " \u204d ",
            "mapped_locations": {
              "D:\\ado": "",
              "D:\\ado2": "",
              "D:\\m\\projects\\MS\\ado.configchange": ""
            },
            "max_depth": 5,
            "style": "agnoster_short"
          },
          "alias": "current path",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .Path }}",
          "foreground": "transparent",
          "background": "#A5C25C",
          "type": "path",
          "style": "diamond",
          "include_folders": [
            "D:\\\\m\\\\projects\\\\MS\\\\ado.*",
            "D:\\\\ado.*"
          ]
        },
        {
          "properties": {
            "folder_separator_icon": " \u204d ",
            "mapped_locations": {
              "D:\\m\\projects\\MS": "~MS",
              "D:\\m\\projects\\SP": "~SP"
            },
            "max_depth": 5,
            "style": "agnoster_short"
          },
          "alias": "special paths",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ .Path }}",
          "foreground": "transparent",
          "background": "#A5C25C",
          "type": "path",
          "style": "diamond",
          "exclude_folders": [
            "D:\\\\m\\\\projects\\\\MS\\\\ado.*",
            "D:\\\\ado.*"
          ]
        },
        {
          "alias": "git properties",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "foreground": "transparent",
          "background": "#67D9F0",
          "type": "git",
          "style": "diamond"
        },
        {
          "alias": "exit code if failed",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if gt .Code 0 }} \uf00d {{ .Meaning }} \uf00d {{ else }} \uf42e {{ end }}",
          "foreground": "#ffffff",
          "background": "#FA2573",
          "type": "exit",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "alias": "right-aligned previous command timing",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .Env.MEASURE_PREVIOUS }}",
          "foreground": "transparent",
          "background": "#008080",
          "type": "text",
          "style": "diamond"
        },
        {
          "alias": "trailing decoration",
          "trailing_diamond": "\u2500\u256e",
          "template": "<,></>",
          "foreground": "transparent",
          "background": "#26c6da",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "alias": "prompt prefix",
          "leading_diamond": "\u2570\u2500",
          "template": "<,></>",
          "foreground": "transparent",
          "background": "#26C6DA",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "trailing_diamond": "\u2500\u256f",
          "template": "<,></>",
          "foreground": "transparent",
          "background": "#26C6DA",
          "type": "text",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "auto_upgrade": true,
  "final_space": true,
  "upgrade": {
    "interval": "24h",
    "auto": true,
    "source": "cdn"
  }
}
